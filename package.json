{
    "version": "0.0.0",
    "name": "mimo-website",
    "description": "Mimo is a DocFX based tool for generating minimalistic, modern documentation sites/blogs.",
    "keywords": [
        "docfx",
        "theme",
        "documentation",
        "blog"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/JeremyTCD/Mimo"
    },
    "homepage": "https://github.com/JeremyTCD/Mimo",
    "bugs": "https://github.com/JeremyTCD/Mimo/issues",
    "license": "CC-BY-4.0",
    "author": "JeremyTCD",
    "files": [
        "dist"
    ],
    "scripts": {
        "serve-dev": "set NODE_ENV=development && yarn run serve",
        "serve-production": "set NODE_ENV=production && yarn run serve",
        "serve": "node tools/serve.js -p ../examples/blog/ -n ../node_modules/",
        "build-example-dev": "set NODE_ENV=development && yarn run build-example",
        "build-example-production": "set NODE_ENV=production && yarn run build-example",
        "build-example": "node tools/build.js -p ../examples/blog/ -n ../node_modules/",
        "build-dist": "node tools/distTools/buildDist.js"
    },
    "dependencies": {
        "@types/clipboard": "^1.5.35",
        "@types/jquery": "^3.2.16",
        "@types/lunr": "^2.1.3",
        "@types/node": "^8.0.53",
        "autoprefixer": "^7.2.4",
        "browser-sync": "^2.18.13",
        "chokidar": "^1.7.0",
        "clipboard": "^1.7.1",
        "cpy": "^6.0.0",
        "css-loader": "^0.28.7",
        "cssnano": "^3.10.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "fork-ts-checker-webpack-plugin": "^0.2.9",
        "glob": "^7.1.2",
        "grunt": "^1.0.1",
        "highlight.js": "^9.12.0",
        "intersection-observer": "^0.5.0",
        "inversify": "^4.11.1",
        "jquery": "^3.2.1",
        "lunr": "^2.1.3",
        "mark.js": "^8.11.1",
        "minimist": "^1.2.0",
        "name-all-modules-plugin": "^1.0.1",
        "node-sass": "^4.5.0",
        "postcss-loader": "^2.0.10",
        "reflect-metadata": "^0.1.12",
        "resize-observer-polyfill": "^1.5.0",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.0",
        "smooth-scroll": "^12.1.5",
        "svg-sprite-loader": "^3.6.2",
        "svgo": "^1.0.3",
        "svgo-loader": "^2.1.0",
        "tippy.js": "^2.1.1",
        "ts-loader": "^3.2.0",
        "twbs-pagination": "^1.3.1",
        "typescript": "^2.1.6",
        "uglifyjs-webpack-plugin": "1.2.4",
        "url-loader": "^0.6.2",
        "webpack": "^3.9.1",
        "webpack-dev-server": "^2.9.7",
        "worker-loader": "^1.1.0"
    }
}
