<ul>
    {{#items}}
    {{^leaf}}
    <li data-level="{{level}}" class="expandable">
    {{/leaf}}
    {{#leaf}}
    <li data-level="{{level}}">
    {{/leaf}}
        {{#topicHref}}
        <a class="topic" data-level="{{level}}" href="{{topicHref}}" name="{{tocHref}}" title="{{name}}">
            <svg>
                <use xlink:href="/styles/sprite.svg#material-design-chevron-right" />
            </svg>
            {{name}}
        </a>
        {{/topicHref}}

        {{^topicHref}}
        <span class="topic" data-level="{{level}}">
            <svg>
                <use xlink:href="/styles/sprite.svg#material-design-chevron-right" />
            </svg>
            {{name}}
        </span>
        {{/topicHref}}
            
        {{^leaf}}
        {{>partials/li}}
        {{/leaf}}
    </li>
    {{/items}}
    </ul>