{{!include(/^styles/.*/)}}
{{!include(/^fonts/.*/)}}
{{!include(favicon.ico)}}
{{!include(logo.svg)}}
{{!include(search-stopwords.json)}}
<!DOCTYPE html>
<!--[if IE]><![endif]-->
<html>
  {{>partials/head}}
  <body>
      {{>partials/header}}
	  {{^_disableBreadcrumb}}
	  {{>partials/breadcrumbs}}
	  {{/_disableBreadcrumb}}
      {{#jr.enableSearch}}
      {{>partials/searchResults}}
      {{/jr.enableSearch}}
	  <div id="main" role="main" >
		  <div class="container">
				{{^_disableToc}}
				{{>partials/sideMenu}}
				{{/_disableToc}}
				<article class="content" id={{uid}}>
				  {{{rawTitle}}}
				  {{^jr.disableMetadata}}
				  {{>partials/metadata}}
				  {{/jr.disableMetadata}}
				  {{#jr.enableArticleList}}
				  {{>partials/articleList}}
				  {{/jr.enableArticleList}}
				  {{^jr.enableArticleList}}
				  {{{conceptual}}}
				  {{/jr.enableArticleList}}
				</article>
			  {{^_disableAffix}}
			  {{>partials/affix}}
			  {{/_disableAffix}}
		  </div>
	  </div>
      {{^_disableFooter}}
      {{>partials/footer}}
      {{/_disableFooter}}
    {{>partials/scripts}}
  </body>
</html>
