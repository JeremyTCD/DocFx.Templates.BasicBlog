<!DOCTYPE html>
<!--[if IE]><![endif]-->
<html>
  {{>partials/head}}
  <body>
      {{>partials/header}}
	  {{^_disableBreadcrumb}}
	  {{>partials/breadcrumbs}}
	  {{/_disableBreadcrumb}}
	  {{^jr.disableLeftMenu}}
	  {{>partials/leftMenuToggle}}
	  {{/jr.disableLeftMenu}}
      {{#jr.enableSearch}}
      {{>partials/searchResults}}
      {{/jr.enableSearch}}
	  <div class="container hide-on-search">
			{{^jr.disableLeftMenu}}
			{{>partials/leftMenu}}
			{{/jr.disableLeftMenu}}
			<main role="main">
				{{>partials/mainArticle}}
				{{#jr.enableSal}}
				{{>partials/sortedArticleList}}
				{{/jr.enableSal}}
			</main>
			{{^jr.disableRightMenu}}
			{{>partials/rightMenu}}
			{{/jr.disableRightMenu}}
	  </div>
      {{^_disableFooter}}
      {{>partials/footer}}
      {{/_disableFooter}}
    {{>partials/scripts}}
  </body>
</html>
