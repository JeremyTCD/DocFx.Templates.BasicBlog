{{!include(/^styles/.*/)}}
{{!include(/^fonts/.*/)}}
{{!include(favicon.ico)}}
{{!include(logo.svg)}}
{{!include(search-stopwords.json)}}
<!DOCTYPE html>

<html>
  {{>partials/head}}
  <body>
      {{>partials/header}}
	  {{^_disableBreadcrumb}}
	  {{>partials/breadcrumbs}}
	  {{/_disableBreadcrumb}}
      {{#bb.enableSearch}}
      {{>partials/searchResults}}
      {{/bb.enableSearch}}
	  <div class="container hide-on-search">
			{{^bb.disableLeftMenu}}
			{{>partials/leftMenu}}
			{{/bb.disableLeftMenu}}
			<main role="main" id={{uid}}>
				{{>partials/mainArticle}}
				{{#bb.insertSal}}
				{{>partials/sortedArticleList}}
				{{/bb.insertSal}}
                {{#bb.enableComments}}
                {{>partials/comments}}
                {{/bb.enableComments}}
			</main>

			{{^bb.disableRightMenu}}
			{{>partials/rightMenu}}
			{{/bb.disableRightMenu}}
	  </div>
      {{^_disableFooter}}
      {{>partials/footer}}
      {{/_disableFooter}}
      {{>partials/scripts}}
  </body>
</html>
