{{!include(/^styles/.*/)}}
{{!include(/^fonts/.*/)}}
<!DOCTYPE html>

<html {{#jtcd_pageID}}id={{jtcd_pageID}}{{/jtcd_pageID}}>
  {{>partials/head}}
  <body>
      {{>partials/header}}
	  {{>partials/breadcrumbs}}
      {{>partials/searchResults}}

	  <div class="container hide-on-search {{#jtcd_disableBreadcrumbs}}breadcrumbs-disabled{{/jtcd_disableBreadcrumbs}}">
			{{>partials/leftMenu}}

			<main role="main" class="{{#jtcd_disableBreadcrumbs}}breadcrumbs-disabled{{/jtcd_disableBreadcrumbs}}">
				{{>partials/article}}
				{{>partials/sortedArticleList}}
                {{>partials/comments}}

			</main>
			{{>partials/rightMenu}}

	  </div>
      {{>partials/footer}}
      {{>partials/scripts}}

  </body>
</html>
