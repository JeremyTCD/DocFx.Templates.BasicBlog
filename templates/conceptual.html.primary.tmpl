<!DOCTYPE html>
<!--[if IE]><![endif]-->
<html>
  {{>partials/head}}
  <body>
      {{>partials/header}}
	  {{^_disableBreadcrumb}}
	  {{>partials/breadcrumbs}}
	  {{/_disableBreadcrumb}}
	  {{^_disableToc}}
	  {{>partials/sideMenuToggle}}
	  {{/_disableToc}}
      {{#jr.enableSearch}}
      {{>partials/searchResults}}
      {{/jr.enableSearch}}
	  <div id="main" role="main" >
		  <div class="container">
			{{^_disableToc}}
			{{>partials/sideMenu}}
			{{/_disableToc}}
			{{>partials/article}}
			{{^_disableAffix}}
			{{>partials/affix}}
			{{/_disableAffix}}
		  </div>
	  </div>
      {{^_disableFooter}}
      {{>partials/footer}}
      {{/_disableFooter}}
    {{>partials/scripts}}
  </body>
</html>
