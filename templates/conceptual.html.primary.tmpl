<html>
  {{>partials/head}}
  <body>
      {{>partials/header}}
	  {{^_disableBreadcrumb}}
	  {{>partials/breadcrumbs}}
	  {{/_disableBreadcrumb}}
	  {{^jr.disableLeftMenu}}
	  {{>partials/leftMenuToggle}}
	  {{/jr.disableLeftMenu}}
      {{#jr.enableSearch}}
      {{>partials/searchResults}}
      {{/jr.enableSearch}}
	  <main role="main" >
		  <div class="container">
			{{^jr.disableLeftMenu}}
			{{>partials/leftMenu}}
			{{/jr.disableLeftMenu}}
			{{>partials/mainArticle}}
			{{^jr.disableRightMenu}}
			{{>partials/rightMenu}}
			{{/jr.disableRightMenu}}
		  </div>
	  </main>
      {{^_disableFooter}}
      {{>partials/footer}}
      {{/_disableFooter}}
    {{>partials/scripts}}
  </body>
</html>
